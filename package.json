{
  "name": "forecast-api",
  "version": "1.0.0",
  "description": "API to provide forecast information, focused on beaches.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lsuzigan/forecastapi.git"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "api"
  ],
  "author": "Lucas Suzigan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lsuzigan/forecastapi/issues"
  },
  "homepage": "https://github.com/lsuzigan/forecastapi#readme",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.14.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "module-alias": "^2.2.2"
  }
}
